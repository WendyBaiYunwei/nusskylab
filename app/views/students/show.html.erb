<% content_for :main_content do %>
  <% javascript 'students.js' %>
  <% if @role.team_id%>
    <%= render 'pending_with_team', locals: { student: @role } %>
  <% elsif @role.team_id.blank? %>
    <%= render 'student_without_team', locals: { student: @role } %>
  <% else %>
    <%= render 'student_with_team', locals: {
      student: @role, role_data: role_data
    } %>
  <% end %>
<% end %>
